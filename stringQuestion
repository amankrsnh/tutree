#include <iostream>
#include<string.h>
#include <algorithm>
using namespace std;

int main()
{
    string a,b;
    cin>>a>>b;
    int len=b.length();
    for(int i=0;i<a.length();++i)
    {
        string temp1,temp=a.substr(i,len);
        temp1=temp;
        sort(temp.begin(),temp.end());
        sort(b.begin(),b.end());
        if(temp.compare(b)==0)
            cout<<"substring '"<<temp1<<"â€™ present at index "<<i<<endl;
    }

    return 0;
}
